{
  "name": "telescope-control",
  "private": true,
  "version": "0.1.0",
  "description": "Telescope control server and UI",
  "scripts": {
    "start": "node server.js",
    "restart": "sh -c \"pkill -f 'node server.js' 2>/dev/null || true; nohup node server.js > server.log 2>&1 &\"",
    "stop": "sh -c \"pkill -f 'node server.js' || true\"",
    "health": "curl -s http://127.0.0.1:3000/health || true"
  },
  "dependencies": {
    "express": "^5.1.0",
    "jpeg-js": "^0.4.4",
    "serialport": "^12.0.0",
    "sharp": "^0.34.4",
    "ws": "^8.18.3"
  }
}
